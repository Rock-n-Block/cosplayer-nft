{
  "name": "cosplayer-nft",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "start": "craco start --host",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"{src,types,scripts}/**/*.{json,{j,t}s{,x},scss,}\"",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"{src,types}/**/*.ts{,x}\"",
    "lint-fix": "npm run format && npm run lint-fix:ts",
    "lint-fix:ts": "eslint \"{src,types}/**/*.ts{,x}\" --fix",
    "storybook": "start-storybook ./public -p 6006",
    "new": "plop --plopfile generators/index.js",
    "test:update": "jest --updateSnapshot ./src",
    "test:watch": "jest ./src",
    "test:coverage": "jest --coverage --watchAll=false ./src",
    "generate-types": "node scripts/generate-types.js",
    "download-schema": "node scripts/download-schema.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "bash -c tsc"
    ]
  },
